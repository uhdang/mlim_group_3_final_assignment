        "      data.train_test_split(week, shopper)\r\n",
        "\r\n",
        "      # data add categories \r\n",
        "      data.add_categories()\r\n",
        "\r\n",
        "      # create features\r\n",
        "      data.create_feature_dict()\r\n",
        "\r\n",
        "      # combine everything\r\n",
        "      X_train, y_train, X_test, y_test = data.make_featured_data()\r\n",
        "\r\n",
        "      X_train_list.append(X_train)\r\n",
