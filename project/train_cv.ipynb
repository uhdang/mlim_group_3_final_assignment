        "# shopper_list = list(range(2000))\r\n",
        "# shopper_chunks = [shopper_list[i:i + 100] for i in range(0, len(shopper_list), 100)]\r\n",
        "shopper_chunks = [list(range(10)), list(range(10,20))]\r\n",
        "\r\n",
        "cv_dict = {\r\n",
        "    'X_train': list(),\r\n",
